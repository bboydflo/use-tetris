{
  "version": "0.3.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish": "npm publish"
  },
  "dependencies": {
    "klona": "^2.0.4",
    "raf": "^3.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "use-tetris",
  "author": "Florin Cosmin Onciu",
  "module": "dist/use-tetris.esm.js",
  "size-limit": [
    {
      "path": "dist/use-tetris.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-tetris.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/raf": "^3.4.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bboydflo/use-tetris.git"
  },
  "bugs": {
    "url": "https://github.com/bboydflo/use-tetris/issues"
  },
  "keywords": ["preact", "react", "react-hooks", "tetris"]
}
